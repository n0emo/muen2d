{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "verbatimModuleSyntax": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "types": [],
    "paths": {
      "muen/*": [ "./*.js" ]
    }
  },
  "include": [
    "**/*.js",
    "**/*.ts",
    "**/*.d.ts"
  ]
}
