{
  "$schema": "https://biomejs.dev/schemas/2.3.10/schema.json",
  "files": {},
  "formatter": {
    "indentStyle": "space",
    "indentWidth": 4,
    "lineWidth": 120,
    "enabled": true,
    "includes": ["./*.json", "./examples/**/*.js", "./typings/**/*.d.ts"]
  },
  "linter": {
    "enabled": true,
    "includes": ["./examples/**/*.js", "./typings/**/*.d.ts"],
    "rules": {}
  },
  "assist": {
    "enabled": false
  },
  "json": {
    "formatter": {
      "indentWidth": 2,
      "trailingCommas": "none"
    }
  },
  "javascript": {
    "formatter": {
      "trailingCommas": "all"
    }
  }
}
